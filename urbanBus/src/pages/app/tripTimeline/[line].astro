---
import LayoutMobile from '../../../layouts/LayoutMobile.astro';
import Timeline from '../../../components/Timeline';

export function getStaticPaths() {
    return [
        {params: {line: 'L1'}},
        {params: {line: 'L2'}},
        {params: {line: 'L3'}},
        {params: {line: 'L4'}},
        {params: {line: 'L5'}},
        {params: {line: 'L6'}},
        {params: {line: 'L7'}},
        {params: {line: 'L8'}},
        {params: {line: 'L9'}},
        {params: {line: 'L10'}},
        {params: {line: 'L11'}},
    ];
}

const {line} = Astro.params;
---

<LayoutMobile title="urbanBus">
    <Timeline line={line} />
</LayoutMobile>